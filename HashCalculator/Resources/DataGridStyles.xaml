<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style
        x:Key="DataGridStyleNormal"
        BasedOn="{StaticResource DataGridBaseStyle}"
        TargetType="DataGrid">
        <Setter Property="RowStyle" Value="{StaticResource DataGridRowStyleNormal}" />
        <Setter Property="CellStyle" Value="{StaticResource DataGridCellStyleNormal}" />
    </Style>
</ResourceDictionary>
